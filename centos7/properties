[COMMON]
FROM=localhost:5000/centos:centos7
TIME_ZONE=America/New_York
ENTRYPOINT=${CATALINA_BASE}/run.sh start ${CATALINA_INSTANCE_NAME} && tail -f ${CATALINA_BASE}/instance_${CATALINA_INSTANCE_NAME}/logs/catalina.out

[JDK_1.8]
JDK_VERSION=8u102
JDK_BUILD=b14
JDK_ARCH=x64

[TOMCAT]
CATALINA_INSTANCE_NAME=1
TOMCAT_RUN_USER=root

[OPEN-SSH]
SSH-USER-PASSWORD=password
SSH-PRIVATE-KEY=